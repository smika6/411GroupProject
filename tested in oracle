/*Works in oracle
1) address-
2) person address-
3) supplier location-
4) person-
5) employee-
6) customer-
7) supplier-
8) Items-
9) hardware items-
10) service items-
11) HW used in service
12) customer purchases
13) deliveries
14) purchase from supplier
15) employee perform service
*/
--create person table
CREATE TABLE person(
person_id INTEGER,
person_name VARCHAR(20),
PRIMARY KEY (person_ID)
);

--create employee table
CREATE TABLE employee(
employee_ID INTEGER,
PRIMARY KEY (employee_ID)
);

--create customer table
CREATE TABLE customer(
customer_id INTEGER,
PRIMARY KEY (customer_id)
);

--create address table
CREATE TABLE address(
street_address VARCHAR(30),
city VARCHAR(30),
lives_in_state VARCHAR(30),
PRIMARY KEY(street_address, city, lives_in_state)
);

--create lives_at table
CREATE TABLE lives_at(
person_id INTEGER,
street_address VARCHAR(30),
city VARCHAR(30),
lives_in_state VARCHAR(30),
PRIMARY KEY(person_id, street_address, city, lives_in_state)
);

--create item table
CREATE TABLE item(
item_id INTEGER,
item_name VARCHAR(20),
item_description VARCHAR(100),
PRIMARY KEY (item_id)
);

--create WSupplier table
CREATE TABLE whole_sale_supplier(
supplier_id INTEGER,
supplier_name VARCHAR(20),
PRIMARY KEY (supplier_id)
);


--create located_at table
CREATE TABLE located_at(
supplier_id INTEGER,
street_address VARCHAR(30),
city VARCHAR(30),
lives_in_state VARCHAR(30),
PRIMARY KEY(supplier_id, street_address, city, lives_in_state)
);

--create hardware table
CREATE TABLE hardware(
hardware_id INTEGER,
PRIMARY KEY (hardware_id)
);

--create service table
CREATE TABLE service_(
service_id INTEGER,
PRIMARY KEY (service_id)
);


